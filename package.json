{
  "name": "cypressvisualimage",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "cypress run --spec 'cypress/e2e/signin-page-snapshot.cy.js'",
    "test:open": "npx cypress run --spec 'cypress/e2e/signin-page-snapshot.cy.js' --headed",
    "test:update-snapshots": "cypress run --env updateSnapshots=true",
    "test:no-fail-on-diff": "cypress run --env failOnSnapshotDiff=false",
    "test:jenkins": "cypress run --config-file cypress.jenkins.config.js",
    "test:with-report": "cypress run --spec 'cypress/e2e/signin-page-snapshot.cy.js' && npm run generate-report",
    "generate-report": "npx mochawesome-merge cypress/reports/*.json > cypress/reports/combined-report.json && npx mochawesome-report-generator cypress/reports/combined-report.json -o cypress/reports",
    "generate-visual-report": "node cypress/support/generate-visual-report.js",
    "test:with-visual-report": "cypress run --spec 'cypress/e2e/signin-page-snapshot.cy.js'; npm run generate-visual-report",
    "open-reports": "node cypress/support/open-reports.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@simonsmith/cypress-image-snapshot": "^10.0.2",
    "cypress": "^14.0.0",
    "cypress-multi-reporters": "^2.0.5",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}
